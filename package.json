  "description": "Based on [https://github.com/truedread/netflix-1080p](https://github.com/truedread/netflix-1080p)",
  "main": "background.js",
  "scripts": {
    "start": "web-ext run -s src",
    "build": "shx mkdir -p dist && shx cp -r src/* dist/",
